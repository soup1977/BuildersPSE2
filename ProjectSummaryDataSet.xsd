                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>/* or whatever ordering you actually want*/
SELECT        p.JBID, pv.VersionName, pv.VersionDate, pv.Description, pv.LastModifiedDate, pv.MondayID, c.CustomerName, s.SalesName, E.EstimatorName, p.ProjectName, p.Address, p.BidDate, p.CreatedDate, p.ProjectID, pv.VersionID, 
                         ProjVersionStatus.ProjVersionStatus
FROM            Projects AS p INNER JOIN
                         ProjectVersions AS pv ON pv.VersionID =
                             (SELECT        TOP (1) VersionID
                               FROM            ProjectVersions AS v
                               WHERE        (ProjectID = p.ProjectID)
                               ORDER BY VersionDate DESC, VersionID DESC) INNER JOIN
                         ProjVersionStatus ON pv.ProjVersionStatusID = ProjVersionStatus.ProjVersionStatusID LEFT OUTER JOIN
                         Customer AS c ON pv.CustomerID = c.CustomerID AND c.CustomerType = 1 LEFT OUTER JOIN
                         Sales AS s ON pv.SalesID = s.SalesID LEFT OUTER JOIN
                         Estimator AS E ON p.EstimatorID = E.EstimatorID
ORDER BY p.BidDate DESC</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
